{
  "expo": {
    "scheme": "nav",
    "name": "Navigation",
    "slug": "Navigation",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to provide navigation services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to provide navigation services even when in the background.",
        "MGLMapboxAccessToken": "pk.eyJ1IjoiYmVyaWNrcyIsImEiOiJjbWVkMmxhdDIwNXdyMmxzNTA3ZnprMHk3In0.hE8cQigI9JFbb9YBHnOsHQ"
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],

      "edgeToEdgeEnabled": true,
      "package": "com.bericks.Navigation",
      "newArchEnabled": false,
      "config": {
        "mapbox": {
          "accessToken": "pk.eyJ1IjoiYmVyaWNrcyIsImEiOiJjbWVkMmxhdDIwNXdyMmxzNTA3ZnprMHk3In0.hE8cQigI9JFbb9YBHnOsHQ"
        }
      }
    },

    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e215f1fb-d717-4bb8-9815-5c2483a5c43d"
      }
    },
    "plugins": [
      "expo-router",
      "./plugins/withKotlinFix.js",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiYmVyaWNrcyIsImEiOiJjbWh1Y2s3MjYxcDVnMm1yMTd1YnJwNnp1In0.S2bV-k3Bq3YZrFGQ4NChfA"
        }
      ],
      "expo-font",
      "expo-asset"
    ]
  }
}
